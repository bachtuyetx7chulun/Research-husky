{
    "plugins": ["prettier", "@typescript-eslint"],
    "extends": ["prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "settings": {
        "import/resolver": {
            "typescript": {
                "alwaysTryTypes": true
            }
        }
    },
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "prettier/prettier": ["error"],
        "no-console": "warn"
    }
}
